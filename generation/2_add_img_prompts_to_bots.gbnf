root ::= Response
Response ::= "{"   ws   "\"prompts\":"   ws   stringlist   "}"
Responselist ::= "[]" | "["   ws   Response   (","   ws   Response)*   "]"
string ::= "\""   ([^"]*)   "\""
boolean ::= "true" | "false"
ws ::= [ \t\n]*
number ::= [0-9]+   "."?   [0-9]*
stringlist ::= "["   ws   "]" | "["   ws   string   (","   ws   string)*   ws   "]"
numberlist ::= "["   ws   "]" | "["   ws   string   (","   ws   number)*   ws   "]"
